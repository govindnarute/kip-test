{
  "name": "kip101-web-backend",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && babel src -d dist -s true",
    "eslint": "eslint -c ./.eslintrc.json src/** migrations/**",
    "lint": "npm run eslint",
    "migrate": "sequelize db:migrate --env",
    "seed": "./node_modules/.bin/sequelize db:seed:all --env",
    "seed-undo": "./node_modules/.bin/sequelize db:seed:undo:all --env",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "dev": "nodemon dist/main.js",
    "watch": "npm run prebuild && babel src -d dist -s true --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.cleveroad.com/commercial/kip101/kip101-web-backend.git"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "awilix": "^4.2.1",
    "aws-sdk": "^2.437.0",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "fast-json-stringify": "^1.14.0",
    "file-type": "^10.10.0",
    "i18n": "^0.8.3",
    "joi": "^14.3.1",
    "joi-to-json-schema": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.23",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.5.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "redis": "^2.8.0",
    "sequelize": "^5.19.6",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.2",
    "@types/redis": "^2.8.12",
    "@types/sequelize": "^4.27.46",
    "@types/winston": "^2.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^1.3.1",
    "nodemon": "^1.19.3",
    "rimraf": "^2.6.3"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
